		FilesProperties.getPrintWriter(path);
		
		
		int counter=0;
		String line="13232ahseeiiiidy";
		
		char [] vowels={'a','e','i','o','u'};
		
		char[]arr = line.toCharArray();
		
		
		
		for(int i=0;i<arr.length;i++){
		      for(int j=0;j<vowels.length;j++){
		        if(arr[i]==vowels[j]){//comparacion
